<?xml version="1.0" encoding="UTF-8"?>
<jms-server xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <service-config>
        <name>jmstest</name>
        <port>21100</port>
        <use-ssl>false</use-ssl>
        <connection-timeout>1000</connection-timeout>
        <client-keepalive-timeout>0</client-keepalive-timeout>
    </service-config>
    <thread-pool>
        <min>40</min>
        <max>40</max>
    </thread-pool>
    <connection-factory>
        <type>queue</type>
        <name>QueueCF</name>
        <export-name>basic/QueueCF</export-name>
    </connection-factory>
    <connection-factory>
        <type>queue</type>
        <name>QueueCF2</name>
        <export-name>basic/QueueCF2</export-name> 
    </connection-factory> 
    <connection-factory>
        <type>topic</type>
        <name>TopicCF</name>
         <export-name>basic/TopicCF</export-name>
   </connection-factory>      
   <connection-factory>
        <type>topic</type>
        <name>TopicCF2</name>
        <export-name>basic/TopicCF2</export-name>
    </connection-factory>
    <connection-factory>
        <type>topic</type>
        <name>TopicCF3</name>
        <export-name>basic/TopicCF3</export-name>
    </connection-factory>  
    <connection-factory>
        <type>topic</type>
        <name>durable_sub_connection_factory</name>
        <export-name>durable_sub_connection_factory</export-name>  
        <client-id>durabl</client-id>
    </connection-factory>

    <destination>
        <type>queue</type>
        <name>QueueExample1</name>
        <export-name>queue/Example1</export-name>    
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>QueueExample2</name>
        <export-name>queue/Example2</export-name>   
        <multiple-receiver>true</multiple-receiver>
    </destination>  
    <destination>
        <type>queue</type>
        <name>QueueExample3</name>
        <export-name>queue/Example3</export-name>    
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>QueueExample4</name>
        <export-name>queue/Example4</export-name>      
        <multiple-receiver>true</multiple-receiver> 
    </destination>  
    
    
    
    <destination>
        <type>topic</type>
        <name>TopicExample1</name>
        <export-name>topic/Example1</export-name>   
    </destination>
    <destination>
        <type>topic</type>
        <name>TopicExample2</name>
        <export-name>topic/Example2</export-name>   
    </destination> 
    <destination>
        <type>topic</type>
        <name>TopicExample3</name>
        <export-name>topic/Example3</export-name>    
    </destination> 
    <destination>
        <type>topic</type>
        <name>TopicExample4</name>
        <export-name>topic/Example4</export-name>    
    </destination> 
     
    <destination>
        <type>queue</type>
        <name>COMMITQUEUE</name>
        <export-name>tx/QueueCommit</export-name> 
        <multiple-receiver>true</multiple-receiver>  
    </destination>
    <destination>
        <type>queue</type>
        <name>ROLLBACKQUEUE</name>
        <export-name>tx/QueueRollback</export-name>
        <multiple-receiver>true</multiple-receiver> 
    </destination>

    <destination>
        <type>queue</type>
        <name>MY_QUEUE1</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>MY_QUEUE2</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>MY_QUEUE3</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>queue</type>
        <name>MY_QUEUE4</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>topic</type>
        <name>MY_TOPIC1</name>
    </destination>
    <destination>
        <type>topic</type>
        <name>MY_TOPIC2</name>
    </destination>
    <destination>
        <type>topic</type>
        <name>MY_TOPIC3</name>
    </destination>
    <destination>
        <type>topic</type>
        <name>MY_TOPIC4</name>
    </destination>
    <destination>
        <type>queue</type>
        <name>testQueue</name>
        <multiple-receiver>true</multiple-receiver>
    </destination>
    <destination>
        <type>topic</type>
        <name>testTopic</name>
    </destination>
    <!-- for jaxws jms transport -->
    <connection-factory>
	<type>queue</type>
	<name>QueueConnectionFactory</name>
	<service>jmstest</service>
    </connection-factory>
    <destination>
	<type>queue</type>
	<name>ExamplesQueue</name>
	<multiple-receiver>true</multiple-receiver>
    </destination>
</jms-server>
