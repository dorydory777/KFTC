<?xml version="1.0" encoding="UTF-8"?><jeus-system xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="6.0" xmlns:ns2="http://java.sun.com/xml/ns/javaee" xmlns:ns3="http://java.sun.com/xml/ns/persistence">
   <node>
      <name>gw-ap2</name>
      <engine-container>
         <name>container1</name>
         <id>28</id>
         <base-port>10031</base-port>
         <command-option>
		-Xms2048m -Xmx2048m -XX:PermSize=512m -XX:MaxPermSize=512m 
		-XX:+DisableExplicitGC -verbose:gc 
		-Xloggc:/home/log/jeusgw/gw-ap2/gclog/container1_gc.log 
		-XX:+PrintGCDetails -XX:+PrintGCTimeStamps 
		-Djeus.servlet.session.enginewideshare=true 
<!--
		-Dpharos.home=/home/PHAROS -Dpharos.agent=gw-ap2-1 
		-javaagent:/home/PHAROS/lib/loader.jar 
		-Dpharos.use.asm=true  
-->
		-XX:+HeapDumpOnOutOfMemoryError 
		-XX:HeapDumpPath=/home/log/jeusgw/gw-ap2/dump
	 </command-option>
         <engine-command>
            <type>servlet</type>
            <name>engine1</name>
         </engine-command>
         <invocation-manager-action>AutoClose</invocation-manager-action>
      </engine-container>
      <class-ftp>true</class-ftp>
      <sequential-start>true</sequential-start>
      <enable-webadmin>true</enable-webadmin>
      <system-logging>
         <name>jeus</name>
         <handler>
            <file-handler>
               <name>handler1</name>
               <level>DEBUG</level>
               <valid-day>1</valid-day>
            </file-handler>
         </handler>
      </system-logging>
   </node>

   <resource>
      <data-source>
         <database>
            <vendor>oracle</vendor>
            <export-name>kftckms</export-name>
            <data-source-class-name>oracle.jdbc.pool.OracleConnectionPoolDataSource</data-source-class-name>
            <data-source-type>ConnectionPoolDataSource</data-source-type>
            <database-name>kmskftc</database-name>
            <port-number>1528</port-number>
            <server-name>gw-db-vip</server-name>
            <user>intraware</user>
            <password>{SEED}RRAdjYi/+qQwdJ9x7R225uoKyn/cQejSLYSRnJsCEpM=</password>
            <property>
               <name>driverType</name>
               <type>java.lang.String</type>
               <value>thin</value>
            </property>
            <connection-pool>
               <pooling>
                  <min>30</min>
                  <max>60</max>
                  <step>1</step>
                  <period>3600000</period>
               </pooling>
               <wait-free-connection>
                  <enable-wait>true</enable-wait>
               </wait-free-connection>
               <check-query>SELECT 1 FROM dual</check-query>
            </connection-pool>
         </database>
      </data-source>
   </resource>

   <application>
      <name>htdocs</name>
      <path>/handy/gw/hip/htdocs</path>
      <deployment-type>COMPONENT</deployment-type>
      <web-component>
         <context-root>/</context-root>
      </web-component>
      <deployment-target>
         <target>
            <engine-container-name>gw-ap2_container1</engine-container-name>
            <web-context-group>
               <name>MyGroup</name>
            </web-context-group>
         </target>
      </deployment-target>
      <classloading>ISOLATED</classloading>
   </application>
   <application>
      <name>wma</name>
      <path>/handy/gw/wma</path>
      <deployment-type>COMPONENT</deployment-type>
      <web-component>
