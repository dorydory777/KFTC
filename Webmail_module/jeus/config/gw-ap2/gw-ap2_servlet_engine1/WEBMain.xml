<?xml version="1.0" encoding="UTF-8"?><web-container xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="6.0" xmlns:ns2="http://java.sun.com/xml/ns/javaee" xmlns:ns3="http://java.sun.com/xml/ns/persistence">
   <context-group>
      <group-name>MyGroup</group-name>
      <webserver-connection>
         <webtob-listener>
            <listener-id>webtob1</listener-id>
            <port>9900</port>
            <output-buffer-size>8192</output-buffer-size>
            <thread-pool>
               <min>50</min>
               <max>100</max>
               <step>10</step>
               <max-idle-time>30000</max-idle-time>
            </thread-pool>
            <webtob-address>localhost</webtob-address>
            <registration-id>MyGroup</registration-id>
	    <webtob-home>/opt/jeusgw/webserver</webtob-home>
         </webtob-listener>
      </webserver-connection>
      <jsp-engine>
         <check-included-jspfile>true</check-included-jspfile>
      </jsp-engine>
      <session-config>
         <distributable>true</distributable>
         <shared>true</shared>
      </session-config>
   </context-group>
   <logging>
      <access-log>
         <handler>
            <file-handler>
               <name>handler1</name>
               <level>INFO</level>
               <valid-day>1</valid-day>
            </file-handler>
         </handler>
      </access-log>
   </logging>
</web-container>
